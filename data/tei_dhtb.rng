         </group>
         <ref name="tei_dhtbatt.global.attributes"/>
         <ref name="tei_dhtbatt.cmc.attributes"/>
         <ref name="tei_dhtbatt.interpLike.attributes"/>
         <empty/>
      </element>
   </define>
